<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>moishd.server.servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>InsertUserServlet</servlet-name>
		<servlet-class>moishd.server.servlets.InsertUserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InsertUserServlet</servlet-name>
		<url-pattern>/InsertUser</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>DeleteUsersServlet</servlet-name>
		<servlet-class>moishd.server.servlets.DeleteUsersServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteUsersServlet</servlet-name>
		<url-pattern>/DeleteUsers</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CookieServlet</servlet-name>
		<servlet-class>moishd.server.servlets.CookieServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CookieServlet</servlet-name>
		<url-pattern>/Cookie</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>GetAllUsersServlet</servlet-name>
		<servlet-class>moishd.server.servlets.GetAllUsersServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetAllUsersServlet</servlet-name>
		<url-pattern>/GetAllUsers</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>GuestbookServlet</servlet-name>
		<servlet-class>moishd.server.servlets.GuestbookServlet</servlet-class></servlet>
	<servlet-mapping>
		<servlet-name>GuestbookServlet</servlet-name>
		<url-pattern>/guestbook</url-pattern></servlet-mapping>
	<servlet>
		<servlet-name>RegistrationServlet</servlet-name>
		<servlet-class>moishd.server.servlets.RegistrationServlet</servlet-class></servlet>
	<servlet-mapping>
		<servlet-name>RegistrationServlet</servlet-name>
		<url-pattern>/registration</url-pattern></servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
