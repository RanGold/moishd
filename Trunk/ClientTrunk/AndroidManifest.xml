<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="moishd.android"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:icon="@drawable/moishd_logo" android:label="@string/app_name" android:theme="@android:style/Theme.Light">
        <activity android:name=".WelcomeScreenActivity" android:label="@string/app_name"
          android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AccountList" android:label="@string/app_name"></activity> 
        <activity android:name=".AuthorizeGoogleAccount" android:label="@string/app_name"></activity>
        <activity android:name=".UsersTabWidget" android:label="@string/app_name"></activity> 
        <activity android:name=".AllOnlineUsersActivity" android:label="@string/app_name"></activity> 
    	<activity android:name=".NearbyUsersActivity" android:label="@string/app_name"></activity> 

    </application>
    <uses-permission android:name="android.permission.GET_ACCOUNTS"></uses-permission>
	<uses-permission android:name="android.permission.USE_CREDENTIALS"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

</manifest> 