<?xml version="1.0" encoding="utf-8"?>
<manifest 
	xmlns:android="http://schemas.android.com/apk/res/android"
	package="moishd.android" 
	android:versionCode="1" 
	android:versionName="1.0">
	
	<permission 
		android:name="moishd.android.permission.C2D_MESSAGE" 
		android:protectionLevel="signature" />
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="moishd.android.permission.C2D_MESSAGE" />
	<uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
	
	<application 
		android:icon="@drawable/moishd_logo"
		android:label="@string/app_name" android:theme="@android:style/Theme.Light">
		<activity android:name=".WelcomeScreenActivity"
			android:label="@string/app_name" android:theme="@android:style/Theme.NoTitleBar">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".AccountList" android:label="@string/app_name"></activity>
		<activity android:name=".AuthorizeGoogleAccount" android:label="@string/app_name"></activity>
		<activity android:name=".UsersTabWidget" android:label="@string/app_name"></activity>
		<activity android:name=".AllOnlineUsersActivity" android:label="@string/app_name" android:launchMode="singleTop"></activity>
		<activity android:name=".NearbyUsersActivity" android:label="@string/app_name"></activity>
		<activity android:name=".games.FastTyping" android:label="@string/app_name"> </activity>
		<activity android:name=".games.youHaveBeenMoishd" android:label="@string/app_name"> </activity>
		<activity android:name=".games.youMoishd" android:label="@string/app_name"> </activity>        
        <activity android:name=".games.FastClick" android:label="@string/app_name"></activity>
		<activity android:name=".games.FastTyping" android:label="@string/app_name"></activity>
		<activity android:name=".games.FastTyping2" android:label="@string/app_name"></activity>
		<activity android:name=".games.FastTyping3" android:label="@string/app_name"></activity>
		<activity android:name=".games.TruthPart" android:label="@string/app_name"></activity>
		<activity android:name=".games.Mixing" android:label="@string/app_name"></activity>
		<activity android:name=".games.Click" android:label="@string/app_name"></activity>
		<activity android:name=".games.Simon" android:label="@string/app_name" android:launchMode="singleTop"></activity>
		<activity android:name=".games.SimonPro" android:label="@string/app_name"></activity>
		<activity android:name=".games.TruthOrDare" android:label="@string/app_name"></activity>
		
		<service android:name=".C2DMReceiver"/>
		<receiver android:name="com.google.android.c2dm.C2DMBroadcastReceiver" 
                  android:permission="com.google.android.c2dm.permission.SEND">
			<!-- Receive the actual message -->
			<intent-filter>
				<action android:name="com.google.android.c2dm.intent.RECEIVE" />
				<category android:name="moishd.android" />
			</intent-filter>
			<!-- Receive the registration id -->
			<intent-filter>
				<action android:name="com.google.android.c2dm.intent.REGISTRATION" />
				<category android:name="moishd.android" />
			</intent-filter>
		</receiver>

	</application>
	<uses-sdk android:minSdkVersion="8"></uses-sdk> 	
				
</manifest> 